<UserControl x:Class="GraduationProject.View.ModeAnalysisView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraduationProject.View"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="1500">

    <UserControl.Resources>
        <Style x:Key="TitleText" TargetType="TextBlock">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="TextAlignment" Value="Center"/>
            <Setter Property="TextWrapping" Value="Wrap"/>
            <Setter Property="Margin" Value="0 0 0 10"/>
        </Style>
        <Style x:Key="SmallText" TargetType="TextBlock">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="FontWeight" Value="Light"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="TextAlignment" Value="Center"/>
            <Setter Property="TextWrapping" Value="Wrap"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
        </Style>
    </UserControl.Resources>
    <ScrollViewer x:Name="ScrollViewerMain">
        <Grid Background="White">
            <StackPanel>
                <Button x:Name="ReturnButton" Background="Transparent" BorderBrush="Transparent" Command="{Binding Return}" HorizontalAlignment="Left">
                    <materialDesign:PackIcon Kind="ArrowLeft" Foreground="Gray" Height="40" Width="auto"/>
                </Button>
                <StackPanel Orientation="Horizontal">
                    <Border Background="#17171a" CornerRadius="10" Margin="10" Padding="20 10" Width="Auto">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid Height="1" Background="Transparent" VerticalAlignment="Bottom">
                                    <Border CornerRadius="1" Background="White"/>
                                </Grid>

                                <TextBlock Text="Общие параметры расчета" Style="{StaticResource TitleText}" VerticalAlignment="Center"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0" Grid.Row="0">
                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Grid.Column="0" Text="Uном, кВ: " Style="{StaticResource SmallText}"/>
                                        <TextBox Grid.Column="1" HorizontalAlignment="Right" Text="{Binding Unom}" MinWidth="100"  Margin="0 5 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Column="1" Grid.Row="0" >
                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Grid.Column="0" Text="δUст, %: " Style="{StaticResource SmallText}"/>
                                        <TextBox Grid.Column="1" MinWidth="100" Text="{Binding Ust}" Margin="0 5 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Column="0" Grid.Row="1">
                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Grid.Column="0" Text="n: " Style="{StaticResource SmallText}"/>
                                        <TextBox Grid.Column="1" MinWidth="100" Text="{Binding N}" Margin="0 5 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Column="1" Grid.Row="1">
                                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Grid.Column="0" Text="m: " Style="{StaticResource SmallText}"/>
                                        <TextBox Grid.Column="1" MinWidth="100" Text="{Binding M}" Margin="0 5 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Border.Triggers>
                            <EventTrigger RoutedEvent="UserControl.Loaded">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity" 
                                         BeginTime="0:0:0.25"
                                         From="0"
                                         To="1"
                                         Duration="0:0:0.5"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" 
                                         BeginTime="0:0:0.25"
                                         From="1800"
                                         To="0"
                                         Duration="0:0:1"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </Border.Triggers>
                        <Border.RenderTransform>
                            <TranslateTransform X="1800" Y="0"/>
                        </Border.RenderTransform>
                    </Border>
                    <Border Background="#17171a" CornerRadius="10" Margin="10" Padding="10" Width="300">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid Height="1" Background="Transparent" VerticalAlignment="Bottom">
                                    <Border CornerRadius="1" Background="White"/>
                                </Grid>

                                <TextBlock Text="Регулирование напряжения в ЦП" Margin="0 0 0 10" Style="{StaticResource TitleText}" TextWrapping="Wrap"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock Text="δUцп max,%" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding DUcpMax}" Margin="0 10 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock Text="δUцп min,%" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding DUcpMin}" Margin="0 10 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Border.Triggers>
                            <EventTrigger RoutedEvent="UserControl.Loaded">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity" 
                                         BeginTime="0:0:0.5"
                                         From="0"
                                         To="1"
                                         Duration="0:0:0.5"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" 
                                         BeginTime="0:0:0.5"
                                         From="1500"
                                         To="0"
                                         Duration="0:0:1"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </Border.Triggers>
                        <Border.RenderTransform>
                            <TranslateTransform X="1500" Y="0"/>
                        </Border.RenderTransform>
                    </Border>

                    <Border Background="#17171a" CornerRadius="10" Margin="10" Padding="10" Width="310">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid Height="1" Background="Transparent" VerticalAlignment="Bottom">
                                    <Border CornerRadius="1" Background="White"/>
                                </Grid>

                                <TextBlock Text="Потери напряжения до потребителя" Style="{StaticResource TitleText}" TextWrapping="Wrap" VerticalAlignment="Center"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock Text="dUнн.у max,%" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding DUnnyMax}" Margin="0 10 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock Text="dUнн.у min,%" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding DUnnyMin}" Margin="0 10 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Border.Triggers>
                            <EventTrigger RoutedEvent="UserControl.Loaded">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity" 
                                         BeginTime="0:0:0.75"
                                         From="0"
                                         To="1"
                                         Duration="0:0:0.5"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" 
                                         BeginTime="0:0:0.75"
                                         From="1000"
                                         To="0"
                                         Duration="0:0:1"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </Border.Triggers>
                        <Border.RenderTransform>
                            <TranslateTransform X="1000" Y="0"/>
                        </Border.RenderTransform>
                    </Border>
                    <Border Background="#17171a" CornerRadius="10" Margin="10" Padding="20 10" Width="Auto">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid Height="1" Background="Transparent" VerticalAlignment="Bottom">
                                    <Border CornerRadius="1" Background="White"/>
                                </Grid>

                                <TextBlock Text="Добавки напряжения" Style="{StaticResource TitleText}" TextWrapping="Wrap"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <StackPanel Orientation="Vertical" Margin="0 0 10 0">
                                        <TextBlock Text="#" Style="{StaticResource SmallText}" Margin="0 0 0 1"/>
                                        <TextBlock Text="1" Style="{StaticResource SmallText}" Margin="0 0 0 2"/>
                                        <TextBlock Text="2" Style="{StaticResource SmallText}" Margin="0 0 0 2"/>
                                        <TextBlock Text="3" Style="{StaticResource SmallText}" Margin="0 0 0 2"/>
                                        <TextBlock Text="4" Style="{StaticResource SmallText}" Margin="0 0 0 2"/>
                                        <TextBlock Text="5" Style="{StaticResource SmallText}" Margin="0 0 0 2"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical" Margin="0 0 10 0">
                                        <TextBlock Text="Ответвление" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding Branch_1}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding Branch_2}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding Branch_3}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding Branch_4}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding Branch_5}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="δUт, %" Style="{StaticResource SmallText}"/>
                                        <TextBox MinWidth="100" Text="{Binding DeltaUBranch_1}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding DeltaUBranch_2}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding DeltaUBranch_3}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding DeltaUBranch_4}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                        <TextBox MinWidth="100" Text="{Binding DeltaUBranch_5}" Padding="3 0 0 0">
                                            <TextBox.Template>
                                                <ControlTemplate>
                                                    <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                        <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                    </Border>
                                                </ControlTemplate>
                                            </TextBox.Template>
                                        </TextBox>
                                    </StackPanel>
                                </StackPanel>
                            </Grid>
                        </Grid>
                        <Border.Triggers>
                            <EventTrigger RoutedEvent="UserControl.Loaded">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity" 
                                         BeginTime="0:0:1"
                                         From="0"
                                         To="1"
                                         Duration="0:0:0.5"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                        <DoubleAnimation Storyboard.TargetProperty="RenderTransform.(TranslateTransform.X)" 
                                         BeginTime="0:0:1"
                                         From="1000"
                                         To="0"
                                         Duration="0:0:1"
                                         FillBehavior="HoldEnd">
                                            <DoubleAnimation.EasingFunction>
                                                <PowerEase Power="3" />
                                            </DoubleAnimation.EasingFunction>
                                        </DoubleAnimation>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </Border.Triggers>
                        <Border.RenderTransform>
                            <TranslateTransform X="1000" Y="0"/>
                        </Border.RenderTransform>
                    </Border>
                </StackPanel>
                <Border BorderBrush="DeepSkyBlue" BorderThickness="0 0 0 5"/>
                <Grid Background="#17171a" Height="Auto">
                    <Border BorderBrush="#333" BorderThickness="0 1"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.15*"/>
                            <ColumnDefinition Width="0.15*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <TextBlock Foreground="White" Text="Начало" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <TextBlock Foreground="White" Text="Конец" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                        </Grid>
                        <Grid Grid.Column="2">
                            <Border BorderBrush="White" BorderThickness="1 0 1 0"/>
                            <DockPanel>
                                <Grid DockPanel.Dock="Top">
                                    <Border BorderBrush="White" BorderThickness="0 1"/>
                                    <TextBlock Foreground="White" Text="Режим наибольших нагрузок" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0">
                                        <TextBlock Foreground="White" Text="Snom, кВА" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="1">
                                        <TextBlock Foreground="White" Text="cos(fi)" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="2">
                                        <TextBlock Foreground="White" Text="P, кВт" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="3">
                                        <TextBlock Foreground="White" Text="Q, кВар" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                </Grid>
                            </DockPanel>
                        </Grid>
                        <Grid Grid.Column="3">
                            <DockPanel>
                                <Grid DockPanel.Dock="Top">
                                    <Border BorderBrush="White" BorderThickness="0 1"/>
                                    <TextBlock Foreground="White" Text="Режим наименьших нагрузок" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0">
                                        <TextBlock Foreground="White" Text="Snom, кВА" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="1">
                                        <TextBlock Foreground="White" Text="cos(fi)" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="2">
                                        <TextBlock Foreground="White" Text="P, кВт" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                    <Grid Grid.Column="3">
                                        <TextBlock Foreground="White" Text="Q, кВар" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="DemiBold" FontFamily="Segoe UI Black" Margin="10"/>
                                    </Grid>
                                </Grid>
                            </DockPanel>
                        </Grid>
                    </Grid>
                </Grid>
                <ListView ItemsSource="{Binding ItemsSourceMaxMin}" Background="Transparent" BorderThickness="0">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <Grid x:Name="ItemsSourceMax">
                                    <Border BorderThickness="0 0 0 1" BorderBrush="#333"/>
                                    <Grid  Margin="0 10">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.15*"/>
                                            <ColumnDefinition Width="0.15*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="0">
                                            <StackPanel Orientation="Vertical" Margin="10 0 0 0">
                                                <Border Background="Gray" CornerRadius="5" HorizontalAlignment="Center" Padding="10 2 10 2">
                                                    <TextBlock Foreground="White" Text="{Binding Path=LoadTransformerMax.N}" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                        </Grid>
                                        <Grid Grid.Column="1">
                                            <StackPanel Orientation="Vertical" Margin="10 0 0 0">
                                                <Border Background="Gray" CornerRadius="5" HorizontalAlignment="Center" Padding="10 2 10 2">
                                                    <TextBlock Foreground="White" Text="{Binding Path=LoadTransformerMax.K}" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                        </Grid>
                                        <Grid Grid.Column="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid Grid.Column="0">
                                                <TextBox Text="{Binding Path=LoadTransformerMax.Snom}" Margin="20 0">
                                                    <TextBox.Template>
                                                        <ControlTemplate>
                                                            <Border CornerRadius="5"  BorderThickness="1" BorderBrush="#FF468AFF" Background="White">
                                                                <ScrollViewer Margin="0" x:Name="PART_ContentHost" />
                                                            </Border>
                                                        </ControlTemplate>
                                                    </TextBox.Template>
                                                </TextBox>
